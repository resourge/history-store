{
	"name": "@resourge/history-store",
	"version": "1.3.3",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"react-native": "./dist/index.native.js",
	"private": false,
	"type": "module",
	"publishConfig": {
		"access": "public"
	},
	"keywords": [
		"javascript",
		"URLSearchParams",
		"react",
		"hooks",
		"typescript",
		"react-hooks",
		"react-search-params"
	],
	"files": [
		"dist"
	],
	"author": "resourge",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/resourge/history-store.git"
	},
	"exports": {
		".": {
			"default": "./dist/index.js"
		},
		"./mobile": {
			"default": "./dist/index.native.js"
		},
		"./utils": {
			"default": "./dist/index.utils.js"
		}
	},
	"typesVersions": {
		"*": {
			"index.d.ts": [
				"./dist/index.d.ts"
			],
			"mobile": [
				"./dist/index.native.d.ts"
			],
			"utils": [
				"./dist/index.utils.d.ts"
			]
		}
	},
	"scripts": {
		"commitizen": "git-cz",
		"commit": "git pull && git add . && npm run commitizen",
		"lint": "eslint \"./src/**/*.{ts,tsx}\"",
		"lint:prod": "cross-env NODE_ENV=production npm run lint",
		"dev": "vite",
		"build": "tsc && vite build",
		"test": "vitest run",
		"test:watch": "vitest",
		"coverage": "vitest run --coverage",
		"semantic-release": "semantic-release"
	},
	"peerDependencies": {
		"react-native": "*",
		"react-native-url-polyfill": "*"
	},
	"peerDependenciesMeta": {
		"react-native": {
			"optional": true
		},
		"react-native-url-polyfill": {
			"optional": true
		}
	},
	"devDependencies": {
		"@babel/core": "^7.25.2",
		"@babel/preset-env": "^7.25.4",
		"@babel/preset-react": "^7.24.7",
		"@fastify/deepmerge": "^2.0.0",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/exec": "^6.0.3",
		"@semantic-release/git": "^10.0.1",
		"@testing-library/jest-dom": "^6.4.6",
		"@testing-library/react": "^16.0.1",
		"@testing-library/user-event": "^14.5.2",
		"@types/jest": "^29.5.13",
		"@types/node": "^22.5.5",
		"@types/react": "^18.3.7",
		"@types/react-dom": "^18.3.0",
		"@vitejs/plugin-react": "^4.3.1",
		"@vitest/coverage-v8": "^2.1.1",
		"app-root-path": "^3.1.0",
		"babel-jest": "^29.7.0",
		"cross-env": "^7.0.3",
		"cz-conventional-changelog": "^3.3.0",
		"eslint-config-resourge": "^1.4.3",
		"glob": "^11.0.0",
		"jest": "^29.7.0",
		"jsdom": "^25.0.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-native": "^0.75.3",
		"react-native-url-polyfill": "^2.0.0",
		"rollup-plugin-cleanup": "^3.2.1",
		"semantic-release": "^24.1.1",
		"ts-jest": "^29.2.5",
		"typescript": "^5.6.2",
		"vite": "^5.4.6",
		"vite-plugin-banner": "^0.8.0",
		"vite-plugin-checker": "^0.8.0",
		"vite-plugin-dts": "^4.2.1",
		"vite-tsconfig-paths": "^5.0.1",
		"vitest": "^2.1.1"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"dependencies": {
		"ts-node": "^10.9.2"
	}
}
