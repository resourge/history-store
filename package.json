{
	"name": "@resourge/history-store",
	"version": "1.1.0",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"private": false,
	"type": "module",
	"publishConfig": {
		"access": "public"
	},
	"keywords": [
		"javascript",
		"URLSearchParams",
		"react",
		"hooks",
		"typescript",
		"react-hooks",
		"react-search-params"
	],
	"files": [
		"dist"
	],
	"author": "resourge",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/resourge/history-store.git"
	},
	"exports": {
		".": {
			"default": "./dist/index.js"
		},
		"./mobile": {
			"default": "./dist/index.native.js"
		}
	},
	"typesVersions": {
		"*": {
			"index.d.ts": [
				"./dist/index.d.ts"
			],
			"mobile": [
				"./dist/index.native.d.ts"
			]
		}
	},
	"scripts": {
		"commitizen": "git-cz",
		"commit": "git pull && git add . && npm run commitizen",
		"lint": "eslint \"./src/**/*.{ts,tsx}\"",
		"lint:prod": "cross-env NODE_ENV=production npm run lint",
		"dev": "vite",
		"build": "tsc && vite build",
		"test": "vitest run",
		"test:watch": "vitest",
		"coverage": "vitest run --coverage",
		"semantic-release": "semantic-release"
	},
	"peerDependencies": {
		"react-native": "*",
		"react-native-url-polyfill": "*"
	},
	"peerDependenciesMeta": {
		"react-native": {
			"optional": true
		},
		"react-native-url-polyfill": {
			"optional": true
		}
	},
	"devDependencies": {
		"@fastify/deepmerge": "^2.0.0",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/exec": "^6.0.3",
		"@semantic-release/git": "^10.0.1",
		"@testing-library/jest-dom": "^6.4.6",
		"@testing-library/react": "^16.0.0",
		"@testing-library/user-event": "^14.5.2",
		"@types/jest": "^29.5.12",
		"@types/node": "^22.5.0",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"@typescript-eslint/eslint-plugin": "^5.52.0",
		"@vitejs/plugin-react": "^4.3.1",
		"app-root-path": "^3.1.0",
		"cross-env": "^7.0.3",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^8.44.0",
		"eslint-config-react-app": "^7.0.1",
		"eslint-config-standard-with-typescript": "^36.0.1",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-import-newlines": "^1.3.4",
		"eslint-plugin-n": "^15.6.1",
		"eslint-plugin-promise": "^6.1.1",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-testing-library": "^5.11.0",
		"eslint-plugin-typescript-sort-keys": "^2.3.0",
		"glob": "^11.0.0",
		"jsdom": "^25.0.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-native": "^0.75.2",
		"react-native-url-polyfill": "^2.0.0",
		"semantic-release": "^24.1.0",
		"typescript": "^5.5.3",
		"vite": "^5.3.3",
		"vite-plugin-banner": "^0.7.1",
		"vite-plugin-checker": "^0.7.2",
		"vite-plugin-dts": "^3.9.1",
		"vite-tsconfig-paths": "^4.3.2",
		"vitest": "^2.0.5"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
